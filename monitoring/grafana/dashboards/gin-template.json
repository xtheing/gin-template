{
  "dashboard": {
    "id": null,
    "title": "Gin Template Dashboard",
    "tags": ["gin-template", "go", "api"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests Total",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{status}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "options": {
          "reduceOptions": [
            {
              "values": false,
              "calcs": [
                "lastNotNull"
              ],
              "fields": ""
            }
          ],
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "displayMode": "list"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "pluginVersion": "6.7.3"
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "options": {
          "reduceOptions": [
            {
              "values": false,
              "calcs": [
                "lastNotNull"
              ],
              "fields": ""
            }
          ],
          "yAxes": [
            {
              "format": "s",
              "label": "Duration"
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "pluginVersion": "6.7.3"
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "options": {
          "reduceOptions": [
            {
              "values": false,
              "calcs": [
                "lastNotNull"
              ],
              "fields": ""
            }
          ],
          "yAxes": [
            {
              "format": "percentunit",
              "label": "Error Rate",
              "max": 1,
              "min": 0
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit"
          }
        },
        "pluginVersion": "6.7.3"
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "options": {
          "reduceOptions": [
            {
              "values": false,
              "calcs": [
                "lastNotNull"
              ],
              "fields": ""
            }
          ],
          "yAxes": [
            {
              "format": "decbytes",
              "label": "Memory"
            }
          ]
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "pluginVersion": "6.7.3"
      }
    ],
    "refresh": "5s",
    "schemaVersion": 27,
    "style": "dark",
    "tags": ["gin-template"],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timezone": "",
    "title": "Gin Template Dashboard",
    "uid": "gin-template-dashboard",
    "version": 1
  }
}
